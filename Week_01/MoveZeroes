public class MoveZeroes{

  public void moveZeroes(int[] nums){
     /*
	   输入: [0,1,0,3,12]
       输出: [1,3,12,0,0]
	 */
     if(nums == null || nums.length == 1){
	     return;
	 }
     int j = 0;
	 for(int i = 0;i<nums.length;i++){
	 
	   if( nums[i] != 0){
	     nums[j] = nums[i];
		 if(j != i){
		   nums[i] = 0;
		 }
	      j++;
	   }
	 
	 }
  
  }
}